{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "noImplicitAny": true,
    "strict": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": false,
    "sourceMap": true,
    "allowJs": false,
    "experimentalDecorators": true,
    "lib": ["esnext", "dom"]
  },
  "exclude": ["node_modules", "*.config.js"]
}
